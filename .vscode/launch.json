{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "admin (web app)",
            "request": "launch",
            "type": "dart",
            "program": "client_app/lib/admin_main.dart",
            "args": ["-d", "chrome", "--web-port", "5000"]
        },
        {
            "name": "client (mobile app)",
            "request": "launch",
            "type": "dart",
            "program": "client_app/lib/main.dart"
        },
        {
            "name": "cycletowork (profile mode)",
            "request": "launch",
            "type": "dart",
            "program": "client_app/lib/main.dart",
            "flutterMode": "profile"
        },
        {
            "name": "cycletowork (release mode)",
            "request": "launch",
            "type": "dart",
            "program": "client_app/lib/main.dart",
            "flutterMode": "release"
        }
    ]
}
